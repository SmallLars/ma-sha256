#define xor(a,b) ((a)&!(b) | !(a)&(b))
#define eq(a,b) ((a)&(b) | !(a)&!(b))

#define INTERNALCARRY ((a0_in & b0_in) | (xor(a0_in, b0_in) & c_in))

NAME = FullAdder2Bit;
INORDER = c_out s1_out a0_in a1_in b0_in b1_in c_in;
OUTORDER = z;

z = eq(s1_out, xor(xor(a1_in, b1_in), INTERNALCARRY)) & eq(c_out, (a1_in & b1_in) | (xor(a1_in, b1_in) & INTERNALCARRY));
